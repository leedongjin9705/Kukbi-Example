package chap12.sec11.exam02;

import java.lang.reflect;
import java.lang.reflect.Constructor;

import chap06.sec06.exam01.Car;

public class RedlextionExample {

	public static void main(String[] args) throws Exception{
		Class clazz = Car.class;
		System.out.println("[생성자 정보]");
		Constructor[] constructors = Clazz.getDeclaredConstructors();
		// getDeclaredConstructors() : 생성자 정보 읽기
		for(Constructor<T> constructors : constructors ) {
			System.out.println(constructors.getName() + "(" +);
			//getName(): 패키지를 포함한 전체 타입 이름
		}
	}

}

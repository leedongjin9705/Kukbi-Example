package chap15.sec07;

import java.util.HashMap;
import java.util.Map;

public class MapExample {

	public static void main(String[] args) {
		Map<String, Integer> map = new HashMap<String, Integer>();
		map.put("blue", 96);
		map.put("hong", 96);
		map.put("white", 96);
		
		String name = null;
		int macScore = 0;
		int totalScore = 0;
		
		Set‹Map. Entry‹String, Integer>> entrySet = map. entrySet() ; for (Map. Entry‹String, Integer> entry : entrySet) { if(entry getValue() › maxscore) {
			name = entry getKey () ;
			maxScore = entry. getValue();
			totalScore += entry• getValue();
			int avgScore = totalScore / map.size(); //57
			System.out.printin("평균 점수:"+ avgScore);
			System.out. println("#] #4: " + maxScore);
			System.out.printin("최고 점수를 받은 아이디: "+ name);

	}

}

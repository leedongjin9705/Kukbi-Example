package chap07.sec06.package2;

import chap07.sec06.package1.A;

public class D extends A {
	// 생성자 선언 
	public D() {
		//A() 생성자 호출
		super(); //부모 클래스 
		
	}
	
	//매소드 선언
	public void method() {
		// A 클래스의 필드값을 변경
		this.field = "value"; //(사용 가능)
		//A 클래스의 메소드를 호출
		this.method1();
	}
 
}

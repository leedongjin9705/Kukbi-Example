package chap06.sec07.exam05;

public class Car {
	//필드 선언
	String comapny = "현대자동차";
	String model;
	String color;
	int maxSpeed;
	
	//생성자 선언
	Car(String model){
		this(model, color, 250);
	}
	
	Car(String model, String color){
		
	}
	
	Car(String model, String color, int maxSpeed){
		this.model = model;
		this.color = color;
		this.maxSpeed = maxSpeed;
		
	}

}

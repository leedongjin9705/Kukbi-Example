package chap06.sec08.exam03;

public class CarExample {

	public static void main(String[] args) {
		// Car 객체 생성
		Car myCar = new Car();
		
		myCar.setGas(5);
		
		if(myCar.isLeftGas()) {
			System.out.println("자동차가 출발합니다");
			//리턴값 없는 run() 메소드 호출
 			myCar.run();
		}
		System.out.println("gsa를 쥡하세요");
	}

}

package chap14.sec06.exam02;

public class ThreadB extends Thread {
	private Workobject workobject;
	
	public ThreadB(Workobject workobject) { // 공유 작업 객체를 받음
		setName("ThreadB"); // 스레드 이름 변경
		this.workobject = workobject;
		
	}
	@Override
	public void run() {
		for(int i; i<10; i++) {
			workobject.methodA(); 
		}
	}

}

package chap08.sec05;

public class Telivision implements RemoteControl{
	// 필드 선언
	private int volume;
	
	@Override // turnOn() 추상 메소드 오버라이딩	
	public void turnOn() {
		System.out.println("TV를 켭니다");
	}
	
	@Override // turnOff() 추상 메소드 오버라이딩	
	public void turnOff() {
		System.out.println("TV를 끕니다");
	}

}

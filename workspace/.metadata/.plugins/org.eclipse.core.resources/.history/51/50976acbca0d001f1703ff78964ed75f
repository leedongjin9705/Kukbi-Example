package chap16.sec01;

public class LambdaExample {

	public static void main(String[] args) {
		action((x,y) -> { // 람다식 1
			int result = x+y;
			Sysyem.out.println("result" + result);
		});
		
		action((x,y) -> { // 람다식 2
			int result = x-y;
			Sysyem.out.println("result" + result);
		});

	}

}

package chap05.sec02;

public class ArrayTest1 {

	public static void main(String[] args) {
		String[] name = new String[4];
		name[0] = "이동진";
		name[1] = "Tom";
		name[2] = "Hillary";
		name[3] = "Hillary2";
		System.out.println("name.length = " + name.length);
		System.out.println("name[1] = " + name[1]);
	}

}

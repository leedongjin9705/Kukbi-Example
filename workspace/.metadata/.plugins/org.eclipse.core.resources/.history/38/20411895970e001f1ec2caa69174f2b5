package chap16.sec06;

public class Person {
	public Member getMember1(Creatable1 creatable1) {
		String id = "winter";
		Member member = creatable1.create(id);
		return member;
	}
	
	public Member getMember1(Creatable2 creatable2) {
		String id = "winter";
		String name = "한겨울";
		Member member = creatable2.create(id, name);
		return member;
	}
	/* Creatable1과 Creatable2 를 매개변수로 갖는 getMember1()
	 * 과 getMember2() 메소드를 작성
 	 * 
	 */

}

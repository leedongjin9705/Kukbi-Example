package chap12.sec04;

public class InExample {

	public static void main(String[] args) throws Exception {
		int speed = 0;
		int KeyCode =0;
		
		while(true) {
			// Enter 키를 치지 않았을 경우에만 실행함
			if(KeyCode != 13 && KeyCode !=10) {
				//유니코드 13, 10은 엔터임
				if(KeyCode == 49) {
					speed++;
				}
			}
		}
		
	}

}
